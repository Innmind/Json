{
    "name": "innmind/json",
    "type": "library",
    "description": "Type safe json encoder/decoder",
    "keywords": ["json"],
    "homepage": "http://github.com/Innmind/Json",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Json/issues"
    },
    "require": {
        "php": "~8.4",
        "ext-json": "*",
        "innmind/immutable": "~5.3"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Json\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~12.0",
        "innmind/static-analysis": "^1.2.1",
        "innmind/coding-standard": "~2.0"
    }
}
